#!/usr/bin/env bash
# Use a while loop to display "Best School" 10 times but for the 9th iteration, displays Best School and then Hi on a new line.
count=0
while [ $count -lt 10 ]
do
    if [ $count -eq 8 ]
    then
        echo -n  "Best School " 
        echo "Hi"  
    else
        echo "Best School"  
    fi

    count=$((count + 1))
done
